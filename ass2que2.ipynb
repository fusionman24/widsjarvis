        "import spacy\n",
        "\n",
        "nlp = spacy.load(\"en_core_web_sm\")\n",
        "\n",
        "phrase = \"SpaCy is a powerful NLP library for tokenization.\"\n",
        "\n",
        "doc = nlp(phrase)\n",
        "\n",
        "print(f\"{'Token':<15}{'Is Space':<10}\")\n",
        "for token in doc:\n",
        "    print(f\"{token.text:<15}{token.whitespace_ != ''}\")\n",
        "\n"
      ],
